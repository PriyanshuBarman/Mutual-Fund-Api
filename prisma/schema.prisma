generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model MutualFund {
  id         Int    @id @default(autoincrement())
  schemeCode Int    @unique
  schemeName String
  isin       String @unique
  kuveraCode String

  @@map("mutual_fund")
}
